{
  "schemaVersion": "2.0",
  "description": "run Powershell script document in JSON",
  "parameters": {
    "Messages": {
      "type": "String",
      "description": "Parameter to say hello world",
      "default": "Hello Json World!!"
    }
  },
  "mainSteps": [
    {
      "action": "aws:runPowerShellScript",
      "name": "example1",
      "inputs": {
        "runCommand": [
          "Write-Output {{Messages}}"
        ]
      }
    }
  ]
}
